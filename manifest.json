
{
  "manifest_version": 3,
  "name": "YAML -- ",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage", "tabs", "webNavigation", "notifications"],
  "host_permissions": [
    "*://*.github.com/*",
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Grab target YAML",
    "default_icon": "16icon.png"
  }
}

